{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "destination": "https://sever-facebook-fake.vercel.app",
      "headers": {
        "host": "sever-facebook-fake.vercel.app",
        "origin": "*",
        "referer": "*"
      }
    }
  ]
}
